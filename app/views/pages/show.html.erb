<% content_for(:html_title) {  "ユーザ一覧" } %>
<br><br>
<%= form_tag users_path, :method => 'get' do %>
  <p>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "Search", :name => nil %>
  </p>
<% end %>

<h1>ユーザ一覧</h1>
<% @users.each do |user| %>
  <li><%= link_to user.username, user.userid %></li>
<% end %>
